year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)

bibentry(bibtype="Manual",
         title = "{MKdescr: Descriptive Statistics}",
         author = person("Matthias", "Kohl"),
         language = "English",
         year = year,
         note = note,
         url = "https://github.com/stamats/MKdescr")
